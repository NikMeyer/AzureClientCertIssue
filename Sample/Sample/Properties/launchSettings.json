{
  "$schema": "http://json.schemastore.org/launchsettings.json",
  "profiles": {
    "Sample": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "test/info",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "dotnetRunMessages": "true",
      "applicationUrl": "https://localhost:5001;http://localhost:5000"
    },
    "Docker-NoSSL": {
      "commandName": "Docker",
      "launchBrowser": true,
      "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}/test/info",
      "publishAllPorts": true,
      "useSSL": false
    },
    "Docker": {
      "commandName": "Docker",
      "launchBrowser": true,
      "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}/test/info",
      "publishAllPorts": true,
      "useSSL": true
    }
  }
}